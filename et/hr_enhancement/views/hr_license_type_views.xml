<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_hr_license_type_tree" model="ir.ui.view">
        <field name="name">hr.license.type.tree</field>
        <field name="model">hr.license.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="description"/>
                <field name="active"/>
            </tree>
        </field>
    </record>

    <record id="view_hr_license_type_form" model="ir.ui.view">
        <field name="name">hr.license.type.form</field>
        <field name="model">hr.license.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="description"/>
                        <field name="active"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
