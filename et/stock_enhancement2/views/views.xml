<odoo>
  <data>
      <record id="stock_picking_action_actualizar_algo" model="ir.actions.server">
        <field name="name">Actualizar Disponible</field>
        <field name="model_id" ref="stock.model_stock_picking"/>
        <field name="binding_model_id" ref="stock.model_stock_picking"/>
        <field name="state">code</field>
        <field name="binding_type">action</field>
        <field name="code">
            action = records.update_available_percent()
        </field>
      </record>

      <record id="my_custom_group_admin" model="res.groups">
          <field name="name">Permisos ADMIN</field>
          <field name="category_id" ref="base.module_category_technical_features"/>
      </record>
  </data>
</odoo>