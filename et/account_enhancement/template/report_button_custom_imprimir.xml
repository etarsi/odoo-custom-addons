<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_buttons_custom" name="Report Buttons Custom">
        <div class="o_report_buttons">
            <button type="button" class="btn btn-primary o_report_print">
                <i class="fa fa-print me-1"></i>
                <span>Imprimir</span>
            </button>

            <button type="button" class="btn btn-secondary o_report_export">
                <i class="fa fa-download me-1"></i>
                <span>Exportar</span>
            </button>
        </div>
    </template>
</odoo>
