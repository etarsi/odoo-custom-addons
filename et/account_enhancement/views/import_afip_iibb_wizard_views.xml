<odoo>
    <record id="view_import_afip_iibb_wizard_form" model="ir.ui.view">
        <field name="name">import.afip.iibb.wizard.form</field>
        <field name="model">import.afip.iibb.wizard</field>
        <field name="arch" type="xml">
            <form string="Importar Alicuotas">
                <group>
                    <field name="url"/>
                    <field name="month" />
                    <field name="year" />
                </group>

                <footer>
                    <button string="Importar" type="object" name="import_data" class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_import_afip_iibb_wizard" model="ir.actions.act_window">
    <field name="name">Importar Alicuotas</field>
    <field name="res_model">import.afip.iibb.wizard</field>
    <field name="view_mode">form</field>
    <field name="target">new</field>
    </record>
</odoo>
