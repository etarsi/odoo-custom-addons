<odoo>
    <record id="action_archive_account_payment_group_act_window" model="ir.actions.act_window">
        <field name="name">Suprimir</field>
        <field name="res_model">account.payment.group.suprimir.wizard</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_account_payment_group_suprimir_wizard_form"/>
        <field name="target">new</field>
        <!-- Para que aparezca en Acción ▾ de account.payment.group -->
        <field name="binding_model_id" ref="account_payment_group.model_account_payment_group"/>
        <field name="binding_type">action</field>
    </record>

    <!-- ACTION ACCOUNT_MOVE_LINE-->
    <record id="action_forzar_reemplazo_product_id_con_nueve" model="ir.actions.server">
        <field name="name">Corrección FC con 9</field>
        <field name="model_id" ref="account.model_account_move_line"/>
        <field name="binding_model_id" ref="account.model_account_move_line"/>
        <field name="binding_view_types">list</field>
        <field name="state">code</field>
        <field name="code">
action = records._forzar_reemplazo_product_id_con_nueve()
        </field>
    </record>

    <record id="action_forzar_reemplazo_product_id_sin_nueve" model="ir.actions.server">
        <field name="name">Corrección FC sin 9</field>
        <field name="model_id" ref="account.model_account_move_line"/>
        <field name="binding_model_id" ref="account.model_account_move_line"/>
        <field name="binding_view_types">list</field>
        <field name="state">code</field>
        <field name="code">
action = records._forzar_reemplazo_product_id_sin_nueve()
        </field>
    </record>
</odoo>
